<div class="row">
  <div id="newsfeed" class="col-lg-12">
    <h4><%= fa_icon "newspaper-o" %>NEWSFEED</h4>
    <div id="activities">
      <% if @activities %>
        <div class="well">
           <%= render_activities(@activities, layout: 'activity') %>
           </div>
      <% else %>
       <%= render('/posts/no_resource', resources: 'Activities') %>
      <% end %>
    </div>
  </div>
</div>